{
  "name": "collin-newman-service",
  "version": "1.0.0",
  "description": "handle the display of reviews for a selected trip-advisor location",
  "main": "index.js",
  "scripts": {
    "kill": "kill -9 $(lsof -i :3000) &>/dev/null | exit 0",
    "test": "jest --verbose --testTimeout=10000 --runInBand --detectOpenHandles --forceExit",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --verbose --coverage --colors",
    "scrape": "node ./dev/unsplashScraper/scrape.js",
    "seed0": "node ./dev/database/postgres/createTables.js",
    "seed1": "node ./dev/database/createUsers.js",
    "seed2": "node ./dev/database/createDestinations.js",
    "seed3": "node ./dev/database/createReviews.js",
    "seed4": "node ./dev/database/createPictures.js",
    "start": "nodemon --signal SIGTERM ./server/server.js",
    "watch": "webpack --watch",
    "webpack": "webpack --json > analysis.json",
    "lint": "./node_modules/.bin/eslint . --ignore-path ./.eslintignore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Slack-Reactor/collin-newman-service.git"
  },
  "keywords": [
    "reviews"
  ],
  "author": "collin-newman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Slack-Reactor/collin-newman-service/issues"
  },
  "homepage": "https://github.com/Slack-Reactor/collin-newman-service#readme",
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "express": "^4.17.1",
    "fontsource-roboto": "^4.0.0",
    "fs": "0.0.1-security",
    "jest": "^25.0.0",
    "make-dir": "^3.1.0",
    "material-ui-dropzone": "^3.5.0",
    "moment": "^2.29.1",
    "mongoose": "^5.11.11",
    "newrelic": "^7.1.1",
    "node-fetch": "^2.6.1",
    "pg": "^8.5.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.2.4",
    "react-material-ui-carousel": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.1.0",
    "file-loader": "^6.2.0",
    "jest-cli": "^26.6.3",
    "nodemon": "^2.0.7",
    "react-addons-test-utils": "^15.6.2",
    "regenerator-runtime": "^0.13.7",
